config_version=5

[application]

config/name="Puzzle Tinker"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[file_customization]

folder_colors={
"res://Audio/": "yellow",
"res://Graphics/": "orange",
"res://Scenes/": "red",
"res://Scripts/": "green"
}

[rendering]

renderer/rendering_method="mobile"
